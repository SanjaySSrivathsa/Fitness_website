                    </div>
                    <h1 id="new">Remaining Sites count for dimension = 600sq.ft : <%= D600 %> </h1>
                    <h1 id="new">Remaining Sites count for dimension = 1200sq.ft : <%= D1200 %> </h1>
                    <h1 id="new">Remaining Sites count for dimension = 2400sq.ft : <%= D2400 %> </h1>
                </div>
                <label for="cars">Dimension = 600 sq.ft :</label>

                <select name="D600" id="D600">
                  <option value="0">0</option>
                  <option value="1">1</option>
                </select>

                <label for="cars">Dimension = 1200 sq.ft :</label>

                <select name="D1200" id="D1200">
                  <option value="0">0</option>
                  <option value="1">1</option>
                </select>
                <label for="cars">Dimension = 2400 sq.ft :</label>

                <select name="D2400" id="D2400">
                  <option value="0">0</option>
                  <option value="1">1</option>
                </select>
                    <button type="submit" class="btn btn-success btn-lg mb-1">Register</button>
      
                  </form>
                </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
